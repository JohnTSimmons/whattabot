DISCORD_TOKEN=

#Guild ID and Client ID are needed by the deploy-commands utility.
GUILD_ID=
CLIENT_ID=

#These are the IDs for the progress report, topic channel groups. Main Text channel is the main text channel we post to.
PROGRESS_REPORT_GROUP_ID=
WEEKLY_TOPIC_GROUP_ID=
MAIN_TEXT_CHANNEL_ID=


# Time data, see https://www.npmjs.com/package/node-schedule, for how to setup the time.
POST_DAY_OF_WEEK=0
POST_HOUR=12
POST_MINUTE=30

#The check post is when the bot will check if an available topic is available to discuss. If not it warns users.
CHECK_DAY_OF_WEEK=6
CHECK_HOUR=12
CHECK_MINUTE=30
TIME_ZONE=US/Central

